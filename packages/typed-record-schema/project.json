{
  "name": "typed-record-schema",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/typed-record-schema/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/typed-record-schema"
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs typed-record-schema {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["coverage/packages/typed-record-schema"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/packages/typed-record-schema"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/typed-record-schema/**/*.ts"]
      }
    }
  },
  "tags": []
}
