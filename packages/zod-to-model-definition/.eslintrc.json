{
    "extends": ["../../.eslintrc.json"],
    "ignorePatterns": ["!**/*"],
    "overrides": [
        {
            "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
            "rules": {}
        },
        {
            "files": ["*.ts", "*.tsx"],
            "rules": {
                "@typescript-eslint/no-non-null-assertion": "off",
                "@typescript-eslint/no-unused-vars": [
                    "warn",
                    {
                        "varsIgnorePattern": "^_$"
                    }
                ]
            }
        },
        {
            "files": ["*.js", "*.jsx"],
            "rules": {}
        }
    ]
}
