{
  "name": "zod-to-model-definitions",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/zod-to-model-definitions/src",
  "projectType": "library",
  "targets": {
    "publish": {
      "command": "node tools/scripts/publish.mjs zod-to-model-definitions {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["coverage/packages/zod-to-model-definitions"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/packages/zod-to-model-definitions"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/zod-to-model-definitions/**/*.ts"]
      }
    }
  },
  "tags": []
}
